{
  "extends": [
      "standard",
      "plugin:react/recommended"
  ],
  "plugins": [
      "import",
      "standard"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
          "jsx": true
      },
      "sourceType": "module"
  },
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "es6": true,
      "jest": true
  },
  "rules": {}
}
